<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "MapPlaceDAO">
    <select id = "getMapPlaceList" resultType = "mapPlace">
		<![CDATA[
        select * from mapplace where mapseq = #{mapSeq}
        ]]>
    </select>
    <select id = "insertMapPlace" resultType = "mapPlace">
		<![CDATA[
        insert into mapplace values(#{mapSeq},#{placeSeq})
        ]]>
    </select>

</mapper>