<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
<properties resource = "db.properties"></properties>
    
    <typeAliases>
        <typeAlias type="com.spring.mau.mapview.MapViewVO" alias="mapView"/>
        <typeAlias type="com.spring.mau.user.UserVO" alias="user"/>
        <typeAlias type="com.spring.mau.userplace.UserPlaceVO" alias="userPlace"/>
        <typeAlias type="com.spring.mau.mapplace.MapPlaceVO" alias="mapPlace"/>
    </typeAliases>
    

    
    <mappers>
        <mapper resource = "mappings/mapview-mapping.xml"/>
        <mapper resource = "mappings/user-mapping.xml"/>
        <mapper resource = "mappings/mapplace-mapping.xml"/>
        <mapper resource = "mappings/userplace-mapping.xml"/>
    </mappers>
</configuration>